    <cm:Communications>
        <Communication id="Ethernet">
            <cm:Metadata>
                <cm:Namespace>Communication.Ethernet</cm:Namespace>
                <cm:Name>Ethernet</cm:Name>
                <cm:Version>1.0.0</cm:Version>
                <cm:Description/>
                <cm:UUID/>
            </cm:Metadata>

            <cm:UIInformation>
                <cm:Content lang="en-US">
                    <DisplayName>Ethernet</DisplayName>
                    <Icon ref="Resource.defaultIcon"/>
                    <cm:Parameters>
                        <cm:Parameter id="ipAddress" displayName="IP Address"/>
                        <cm:Parameter id="portNumber" displayName="Port Number"/>
                    </cm:Parameters>
                </cm:Content>
                <cm:Content lang="zh-CN">
                    <DisplayName>以太网</DisplayName>
                    <cm:Parameters>
                        <cm:Parameter id="ipAddress" displayName="IP地址"/>
                        <cm:Parameter id="portNumber" displayName="端口号"/>
                    </cm:Parameters>
                </cm:Content>
            </cm:UIInformation>

            <cm:Data>
                <cm:Content>
                    <cm:Parameters>
                        <cm:Parameter id="ipAddress" type="IPv4" defaultValue="127.0.0.1"/>
                        <!-- Port number shall be in range 1025~65535, RFC 3232 <= 1024 is not allowed -->
                        <cm:Parameter id="portNumber" type="UInt16" defaultValue="10000"/>
                    </cm:Parameters>
                </cm:Content>
            </cm:Data>

            <cm:Resource>
                <cm:Content id="defaultIcon" type="Icon" format="SVG">
                    <![CDATA[
                    ]]>
                </cm:Content>
            </cm:Resource>
        </Communication>

