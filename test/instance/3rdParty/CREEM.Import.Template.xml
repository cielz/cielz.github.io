<cm:CIMI xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bs="http://creem.siemens.com/schemas/base" xmlns:cim="http://creem.siemens.com/schemas/cim" xmlns:cm="http://creem.siemens.com/schemas/creem" xmlns="http://creem.siemens.com/schemas/creem" state="Alpha" version="0.0.1" lang="en-US">
	<!-- Optional -->
	<cm:FBLibrary>
		<cm:Metadata>
			<cm:Namespace>CREEM.FBLibrary.Instance</cm:Namespace>
			<cm:Name>FBLibrary</cm:Name>
			<cm:Version>1.0.0</cm:Version>
			<cm:Description/>
			<cm:UUID/>
		</cm:Metadata>
		<cm:UIInformation/>
		<cm:Data>
			<cm:Content>
				<cm:Library id="defaultFBLibrary">
					<cm:Section id="logicalNode" isBindDevice="true">
						<cm:FunctionBlock ref="[Manufacturer.Category.FBName]"/>
					</cm:Section>
				</cm:Library>
			</cm:Content>
		</cm:Data>
		<cm:Resource/>
	</cm:FBLibrary>
	<!-- Name the id of the FB by Namespace format Manufacturer.Category.FBName -->
	<cm:FunctionBlock id="[Manufacturer.Category.FBName]">
		<cm:Metadata>
			<cm:Namespace>CREEM.FB.{id}</cm:Namespace>
			<cm:NamespaceURI/>
			<cm:Name>[FBName]</cm:Name>
			<cm:Version>1.0.0</cm:Version>
			<cm:Description/>
			<!-- ### Generated by CREEM Editor ### -->
			<xs:annotation>
				<xs:documentation>Defines complex data types (structs, range types, enumerations, ...) that may be used in the parameters section or by other types (a member of a struct may in turn be an enumeration type etc.)</xs:documentation>
			</xs:annotation>
			<cm:UUID/>
			<cm:Certification/>
			<cm:Organization>
				<cm:OrganizationName>Siemens AG.</cm:OrganizationName>
				<!-- ### Generated by CREEM Editor ### -->
				<cm:DigitalSignature/>
			</cm:Organization>
		</cm:Metadata>
		<cm:UIInformation>
			<!-- ISO639_ISO3166 -->
			<cm:Content lang="en_US">
				<DisplayName>[FB Name]</DisplayName>
				<Description>[FB Description]</Description>
				<Icon ref="Resource.defaultIcon"/>
				<!-- <Diagram location="{id}.json"/> -->
				<!-- <Dialog location="index.html"/> -->
			</cm:Content>
			<cm:Content lang="zh_CN">
				<DisplayName>[功能块中文名]</DisplayName>
				<Description>[功能块中文描述]</Description>
			</cm:Content>
		</cm:UIInformation>
		<cm:Data>
			<cm:Content>
				<cm:Executor location="{id}.py"/>
				<cm:Category type="LogicalNode"/>
				<cm:PhysicalNode type="Device">
					<cm:DeviceDescription ref="Robotic.UniversalRobots.UR5"/>
					<cm:DeviceDescription ref="Robotic.UniversalRobots.UR5E"/>
				</cm:PhysicalNode>
				<cm:Input>
					<cm:Port id="parameter1" type="UNECE" unit="mm" defaultValue="0"/>
					<cm:Port id="reference" type="Ref"/>
				</cm:Input>
				<cm:Output>
					<cm:Port id="status" type="StatusCode"/>
				</cm:Output>
			</cm:Content>
		</cm:Data>
		<cm:Resource>
			<!-- Parse FB SVG Icon here, otherwise it will show default FB icon below -->
			<cm:Content id="defaultIcon" type="Icon" format="SVG">
			<![CDATA[<svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 396.8 396.8" style="enable-background:new 0 0 396.8 396.8;" xml:space="preserve"><g><path d="M393.6,140.7L206.4,23.9c-1.6-0.8-4-0.8-6.4,0L3.2,138.3c-1.6,0.8-3.2,3.2-3.2,4.8v112c0,2.4,0.8,4,3.2,4.8l192,112.8 c0.8,0.8,1.6,0.8,3.2,0.8c0.8,0,2.4,0,3.2-0.8l192-112c1.6-0.8,3.2-3.2,3.2-4.8V145.5C396.8,143.1,396,141.5,393.6,140.7z M208.8,39.9l170.4,104.8l-83.2,48l-87.2-53.6V39.9z M196.8,39.9v99.2l-94.4,53.6l-84.8-48.8L196.8,39.9z M192,356.7L12,251.9 v-98.4l180,104.8V356.7z M198.4,248.7l-84-48.8l88-50.4l80.8,50.4L198.4,248.7z M384.8,251.9l-180,104.8v-97.6l180-104V251.9z"/></g></svg>]]>
			</cm:Content>
		</cm:Resource>
	</cm:FunctionBlock>
</cm:CIMI>
